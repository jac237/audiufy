{
  "version": 2,
  "name": "audiufy",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ],
  "alias": "audiufy"
}
